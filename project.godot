Context="*res://Classes/Utility/Context.tscn"
ProjectFileManager="*res://Classes/Utility/ProjectFileManager.tscn"

[display]

window/size/viewport_width=1280
window/size/viewport_height=720

[dotnet]

project/assembly_name="Osu Timer"
